{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "sv-custom-blocks/quarterly-goals-generator",
	"version": "0.1.0",
	"title": "Quarterly goals Generator",
	"category": "widgets",
	"icon": "star-filled",
	"description": "Generate quarterly goals based on given smart goal.",
	"attributes": {
		"sv_cb_qgoals_assistant_id": {
			"type": "string",
			"default": ""
		},
		"sv_cb_qgoals_show_smart_goal": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_show_timeframe": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_show_assumptions": {
			"type": "boolean",
			"default": false
		},
		"sv_cb_qgoals_show_resources": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_show_risks": {
			"type": "boolean",
			"default": false
		},
		"sv_cb_qgoals_show_kpis": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_show_stages": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_show_actions": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_show_totals": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_allow_editing": {
			"type": "boolean",
			"default": true
		},
		"sv_cb_qgoals_group_actions_by_quarter": {
			"type": "boolean",
			"default": true
		}
	},
	"example": {},
	"supports": {
		"html": false,
		"anchor": true
	},
	"textdomain": "sv-custom-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}